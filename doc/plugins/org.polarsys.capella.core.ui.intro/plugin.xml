<plugin>
   <extension
         point="org.eclipse.ui.intro.configExtension">
         <configExtension
               configId="org.eclipse.ui.intro.universalConfig"
               content="intro/firststeps-actions.xml">
         </configExtension>
         <configExtension
               configId="org.eclipse.ui.intro.universalConfig"
               content="intro/firststeps-help.xml">
         </configExtension>
         <configExtension
               configId="org.eclipse.ui.intro.universalConfig"
               content="intro/firststeps-doc.xml">
         </configExtension>
         <configExtension
               configId="org.eclipse.ui.intro.universalConfig"
               content="intro/firststeps-bug.xml">
         </configExtension>
         <action
               name="createCapellaProjectCommand"
               replaces="runAction?standby=true&amp;pluginId=org.polarsys.capella.core.ui.intro&amp;class=org.polarsys.capella.core.ui.intro.actions.CreateCapellaProjectAction">
         </action>
         <action
               name="importCapellaProjectCommand"
               replaces="runAction?standby=true&amp;pluginId=org.polarsys.capella.core.ui.intro&amp;class=org.polarsys.capella.core.ui.intro.actions.ImportCapellaProjectAction">
         </action>
   </extension>
</plugin>
