                    <parameters name="context" value="&lt;% $element%>"/>
                    <parameters name="scope" value="&lt;%getAvailablePhysicalLinksToInsert($diagram)%>"/>
                    <parameters name="initialSelection" value="&lt;%$displayedLinks%>"/>
