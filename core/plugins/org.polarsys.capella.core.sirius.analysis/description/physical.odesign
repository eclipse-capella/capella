                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%createFunctionalChain($views)%>">
                  <subModelOperations xsi:type="tool:If" conditionExpression="&lt;%current.isA(&quot;FunctionalChain&quot;)%>">
