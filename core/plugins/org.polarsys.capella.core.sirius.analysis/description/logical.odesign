                    <subModelOperations xsi:type="tool:If" conditionExpression="&lt;% !$containerTarget.isA(&quot;BlockArchitecture&quot;) %>">
                      <subModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%$containerType%>">
