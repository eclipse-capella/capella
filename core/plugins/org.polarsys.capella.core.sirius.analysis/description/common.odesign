                        <subModelOperations xsi:type="tool_1:If" conditionExpression="&lt;%ownedPhysicalComponent.nSize()==0%>">
                          <subModelOperations xsi:type="tool_1:CreateInstance" typeName="PhysicalComponent" referenceName="ownedPhysicalComponent" variableName="comp">
                            <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="&lt;%creationService%>"/>
                          </subModelOperations>
                        </subModelOperations>
                      </subModelOperations>
                      <subModelOperations xsi:type="tool_1:If" conditionExpression="&lt;%isA(&quot;EPBSArchitecture&quot;)%>">
                        <subModelOperations xsi:type="tool_1:If" conditionExpression="&lt;%false &amp;&amp; ownedConfigurationItem.nSize()!=0%>">
                          <subModelOperations xsi:type="tool_1:If" conditionExpression="&lt;%ownedConfigurationItemPkg.nSize()!=0%>">
                            <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="&lt;%setVariableA(&quot;owner&quot;, ownedConfigurationItemPkg.nGet(0))%>"/>
                          </subModelOperations>
                          <subModelOperations xsi:type="tool_1:If" conditionExpression="&lt;%ownedConfigurationItemPkg.nSize()==0%>">
                            <subModelOperations xsi:type="tool_1:CreateInstance" typeName="ConfigurationItemPkg" referenceName="ownedConfigurationItemPkg" variableName="owner">
