                          <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="&lt;%creationService%>"/>
                          <subModelOperations xsi:type="tool_1:SetValue" featureName="nature" valueExpression="&lt;%$natureParent%>"/>
