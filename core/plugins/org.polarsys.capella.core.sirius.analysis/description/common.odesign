                      <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="Receive Call &lt;% $call.name%>"/>
                      <subModelOperations xsi:type="tool_1:SetValue" featureName="coveredInstanceRoles" valueExpression="&lt;%$targetIR%>"/>
                      <subModelOperations xsi:type="tool_1:If" conditionExpression="&lt;% $messageEndAfter != null %>">
                        <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="&lt;%moveEndOnScenario($messageEndAfter.semanticEnd)%>"/>
                        <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="&lt;%moveEndOnInstanceRole($messageEndAfter.semanticEnd)%>"/>
                      </subModelOperations>
                      <subModelOperations xsi:type="tool_1:If" conditionExpression="&lt;% $messageEndAfter == null %>">
                        <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="&lt;%moveEndOnBeginingOfScenario%>"/>
