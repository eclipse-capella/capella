	  <extension point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.polarsys.capella.core.refinement.commands.ui.expression.transitionVisibilityModelElementExpression">
             <with variable="selection">
                    <and>
                       <iterate>
                             <instanceof
                                   value="org.polarsys.capella.common.data.modellingcore.ModelElement">
                             </instanceof>
                       </iterate>
                    </and>
             </with>
      </definition>
   </extension>
   
   <extension point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.polarsys.capella.core.refinement.commands.ui.expression.transitionVisibilityDiagramElementExpression">
               <with
                  variable="selection">
               <and>
                  <iterate>
                       <or>
                        <instanceof
                              value="org.eclipse.sirius.diagram.edit.api.part.IDiagramElementEditPart">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.sirius.diagram.edit.api.part.IDDiagramEditPart">
                        </instanceof>
                       </or>
                  </iterate>
               </and>
            </with>
      </definition>
   </extension>
	  
	  
	  
	  
