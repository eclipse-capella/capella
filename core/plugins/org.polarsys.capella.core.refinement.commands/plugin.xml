	      <!--   Refinement transitions diagram --> 
	     <extension
         point="org.eclipse.ui.menus">
      
          <menuContribution  allPopups="true"
                locationURI="popup:capella.project.explorer.menu.transitions.diagram.preferences?after=additions">
             <command  commandId="org.polarsys.capella.core.refinement.commands.actions.scenarioRefinementAction"  style="push" icon="platform:/plugin/org.polarsys.capella.core.ui.resources/icons/full/obj16/capella_process.gif" >
             <visibleWhen checkEnabled="false">
                <with variable="selection">
                    <iterate>
                            <and>
                            <reference definitionId="org.polarsys.capella.core.refinement.commands.ui.expression.transitionVisibilityDiagramElementExpression"/>
                                
                                <or>
                                <test
                                      forcePluginActivation="true"
                                      property="org.polarsys.capella.core.platform.sirius.ui.graphicalLayerOfWithOutDataFlowScenario"
                                      value="SystemAnalysis">
                                </test>
                                <test
                                        forcePluginActivation="true"
                                      property="org.polarsys.capella.core.platform.sirius.ui.graphicalLayerOfWithOutDataFlowScenario"
                                      value="LogicalArchitecture">
                                </test>
                                <test
                                      forcePluginActivation="true"
                                      property="org.polarsys.capella.core.platform.sirius.ui.graphicalLayerOfWithOutDataFlowScenario"
                                      value="PhysicalArchitecture">
                                </test>
                             </or>
                            </and>
                    </iterate>
                </with>
             </visibleWhen> 
             </command> 
          </menuContribution>
      
      </extension>
      
      
   <!-- Refinement transitions Project Explorer -->
   <extension
         point="org.eclipse.ui.menus">
          <menuContribution  allPopups="true"
                locationURI="popup:capella.project.explorer.menu.transitions.preferences?after=additions">
             <command  commandId="org.polarsys.capella.core.refinement.commands.actions.scenarioRefinementAction"  style="push" icon="platform:/plugin/org.polarsys.capella.core.ui.resources/icons/full/obj16/capella_process.gif" >
             <visibleWhen checkEnabled="false">
                <with variable="selection">
                    <iterate>
                            <and>
                               <reference definitionId="org.polarsys.capella.core.refinement.commands.ui.expression.transitionVisibilityModelElementExpression"/>
                                <or>
                                    <test
                                          forcePluginActivation="true"
                                          property="org.polarsys.capella.core.platform.sirius.ui.layerOfWithOutDataFlowScenario"
                                          value="SystemAnalysis">
                                    </test>
                                    <test
                                          forcePluginActivation="true"
                                          property="org.polarsys.capella.core.platform.sirius.ui.layerOfWithOutDataFlowScenario"
                                          value="PhysicalArchitecture">
                                    </test>
                                    <test
                                          forcePluginActivation="true"
                                          property="org.polarsys.capella.core.platform.sirius.ui.layerOfWithOutDataFlowScenario"
                                          value="LogicalArchitecture">
                                    </test>
                
                             </or>
                            </and>
                    </iterate>
                </with>
             </visibleWhen> 
             </command> 
          </menuContribution>
      
      </extension>
      
      <extension point="org.eclipse.ui.commands">
