        </propertyBinding>
        
      </bindings>
      
      
      
      <bindings properties="org.polarsys.capella.common.re.createRec">      
        
        
        <propertyBinding 
            property="locationSource"
            renderer="none">
        </propertyBinding>
      
        <propertyBinding 
            property="itarget"
            renderer="none">
        </propertyBinding>
        
        <propertyBinding 
            property="isource"
            renderer="none">
        </propertyBinding>
        
        <propertyBinding 
            property="targetName"
            renderer="targetTextRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="sourceName"
            renderer="none">
        </propertyBinding>
        
      </bindings>  
                      
      <bindings properties="org.polarsys.capella.common.re.updateDef">
        
        <propertyBinding 
            property="itarget"
            renderer="replicableElementRenderer">
        </propertyBinding>
      
        <propertyBinding 
            property="targetName"
            renderer="targetTextRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="isource"
            renderer="replicaRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="locationSource"
            renderer="none">
        </propertyBinding>
        
      </bindings>
      
      
      
      <bindings properties="org.polarsys.capella.common.re.updateCur">
        
        <propertyBinding 
            property="itarget"
            renderer="replicableElementRenderer">
        </propertyBinding>
      
        <propertyBinding 
            property="targetName"
            renderer="targetTextRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="isource"
            renderer="none">
        </propertyBinding>
        
       <propertyBinding 
            property="sourceName"
            renderer="none">
       </propertyBinding>
       
       <propertyBinding 
            property="locationTarget"
            renderer="none">
       </propertyBinding>
      
       <propertyBinding 
            property="locationSource"
            renderer="none">
       </propertyBinding>
        
      </bindings>
      
      
      
      <bindings properties="org.polarsys.capella.common.re.re2rpl">
        
        <propertyBinding 
            property="locationSource"
            renderer="none">
        </propertyBinding>
      
      
        <propertyBinding 
            property="currentCompliancy"
            renderer="browseImageRenderer">
        </propertyBinding>
        
      </bindings>
      
      
      <bindings properties="org.polarsys.capella.common.re.createReplica">
        
        <propertyBinding 
            property="isource"
            renderer="replicableElementRenderer">
            <!-- ReplicableElement, no edit, browse allowed -->
        </propertyBinding>
      
        <propertyBinding 
            property="targetContent"
            renderer="browseRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="suffix"
            renderer="textRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="targetName"
            renderer="targetTextRenderer">
        </propertyBinding>
        
        
        <propertyBinding 
            property="targetContent"
            renderer="replicaContentRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="targetContentLocation"
            renderer="replicaContentLocationRenderer">
        </propertyBinding>
        <propertyBinding
              property="readOnly"
              renderer="checkboxRenderer">
        </propertyBinding>
        <propertyBinding
              property="parentLocatorOption"
              renderer="optionRenderer">
        </propertyBinding>
        
      </bindings>       
      
      <bindings properties="org.polarsys.capella.common.re.updateReplica">
      
      
        <propertyBinding 
            property="isource"
            renderer="uneditReTextRenderer">
            <!-- ReplicableElement, name edit not allowed -->
        </propertyBinding>
      
        
        <propertyBinding 
            property="itarget"
            renderer="reTextRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="suffix"
            renderer="textRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="targetName"
            renderer="targetTextRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="targetContent"
            renderer="replicaContentRenderer">
        </propertyBinding>
        
        <propertyBinding 
            property="locationSource"
            renderer="none">
        </propertyBinding>
        
        <propertyBinding 
            property="locationTarget"
            renderer="none">
        </propertyBinding>
        
        <propertyBinding 
            property="targetContentLocation"
            renderer="replicaContentLocationRenderer">
        </propertyBinding>
        <propertyBinding
              property="readOnly"
              renderer="checkboxRenderer">
        </propertyBinding>
        <propertyBinding
              property="parentLocatorOption"
              renderer="optionRenderer">
        </propertyBinding>
      </bindings>
            
