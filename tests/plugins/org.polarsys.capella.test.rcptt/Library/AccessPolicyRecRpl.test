get-view "Project Explorer" | get-tree | get-menu "New/Capella Library" | click
with [get-window "New Capella Library"] {
    get-editbox | set-text LibProjectSuper
    get-button Finish | click
}

get-view "Project Explorer" | get-tree | get-menu "New/Capella Library" | click
